{
  "name": "fast-feet-api",
  "version": "0.0.1",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "prestart:dev": "docker compose --env-file .env up -d fast-feet-api-postgresql",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:build": "docker build -t fast-feet-api:latest -t fast-feet-api:$(git rev-parse --short HEAD) .",
    "docker:build:prod": "docker build --target production -t fast-feet-api:latest -t fast-feet-api:$(git rev-parse --short HEAD) .",
    "format": "pnpm exec biome format --write",
    "lint": "pnpm exec biome lint --write",
    "check": "pnpm exec biome check",
    "check:fix": "pnpm exec biome check --write",
    "check:type": "pnpm exec tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.ts",
    "verify": "git pull origin $(git branch --show-current) && pnpm test && pnpm check:fix && pnpm check:type"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "unplugin-swc": "^1.5.9",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@faker-js/faker": "^10.2.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "gerador-validador-cpf": "^6.3.0",
    "globals": "^16.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.15"
  }
}
